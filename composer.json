{
    "name": "percymamedy/laravel-accessory",
    "description": "Collection of Traits and Helper methods for Laravel 5",
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "~5.0",
        "mockery/mockery": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Accessory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Accessory\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
